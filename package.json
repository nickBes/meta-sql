{
  "name": "meta-sql",
  "version": "0.1.0",
  "description": "Tools for processing sql queries metadata",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.4",
    "typescript": "5.8.3",
    "@types/bun": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.17",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": ["sql", "metadata", "database", "query-analysis"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/meta-sql.git"
  },
  "license": "MIT",
  "module": "index.ts",
  "type": "module"
}
