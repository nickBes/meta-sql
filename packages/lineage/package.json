{
  "name": "@meta-sql/lineage",
  "version": "0.0.3",
  "description": "Library for processing lineage from SQL",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickBes/meta-sql.git",
    "directory": "packages/lineage"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "bun test",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@meta-sql/eslint-config": "workspace:*",
    "@meta-sql/typescript-config": "workspace:*",
    "eslint": "^9.30.0",
    "tsup": "^8.0.2",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@meta-sql/open-lineage": "0.1.0"
  },
  "keywords": [
    "sql",
    "lineage",
    "parser",
    "database"
  ],
  "peerDependencies": {
    "node-sql-parser": "^5.3.10"
  }
}
